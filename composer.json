    "description": "Weirweb Engine",
    "version": "0.1",
    "require-dev": {
        "phpstan/phpstan": "^0.12.81"
    },
    "autoload": {
        "psr-4": {
            "Engine\\": "src/"
        }
    },
    "require": {
        "weirweb/engine-page": "@dev",
        "weirweb/engine-routing": "@dev"
    },
    "repositories": [
